  <body>
    
     <a id="top" name="top"></a>
 
<div class="topnav" id="myTopnav">
			<a href="/" class="active">Discord4Bots</a>
      <a href="/bots">Bots</a>
			<a href="/api">API</a>
			<a href="/about">About Us</a>
      <a href="https://discord.gg/fWfPerr">Support</a>
			<% if (!user) { %>
      <a href="/login">Login</a>
      <% } else { %>
      <a href="/addbot">Add Bot</a>
			<a href="/logout">Sign out</a>
			<a href="/user/<%=user.id%>">
      <img src="https://cdn.discordapp.com/avatars/<%=user.id%>/<%=user.avatar%>.gif?size=32" width="25" height="25" class="d-inline-block align-middle rounded-circle" alt="User Avatar">
      <%=user.username%>
      </a>
      <% if (bot.authorities.includes(user.id) === true) { %>
      <a href="/authorized">Authorized Panel</a>
      <% } %>
      <% } %>	
			<a href="javascript:void(0);" class="icon" onclick="myFunction()">
				<i class="fa fa-bars"></i>
			</a>
